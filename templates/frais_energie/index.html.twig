{% extends 'base1.html.twig' %}

{% block title %}FraisEnergie index{% endblock %}

{% block body %}
    <h1>FraisEnergie index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Montant</th>
                <th>Taux_Energie</th>
                <th>Date</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for frais_energie in frais_energies %}
            <tr>
                <td>{{ frais_energie.id }}</td>
                <td>{{ frais_energie.montant }}</td>
                <td>{{ frais_energie.tauxEnergie }}</td>
                <td>{{ frais_energie.date ? frais_energie.date|date('Y-m-d') : '' }}</td>
                <td>
                    <a href="{{ path('app_frais_energie_show', {'id': frais_energie.id}) }}">show</a>
                    <a href="{{ path('app_frais_energie_edit', {'id': frais_energie.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_frais_energie_new') }}">Create new</a>
{% endblock %}
