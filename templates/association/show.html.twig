{% extends 'base1.html.twig' %}

{% block title %}Association
{% endblock %}

{% block body %}

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ association.id }}</td>
			</tr>
			<tr>
				<th>Nom</th>
				<td>{{ association.nom }}</td>
			</tr>
			<tr>
				<th>Adresse</th>
				<td>{{ association.adresse }}</td>
			</tr>
			<tr>
				<th>Rib</th>
				<td>{{ association.rib }}</td>
			</tr>
			<tr>
				<th>Email</th>
				<td>{{ association.email }}</td>
			</tr>
		</tbody>
	</table>

	<a class="btn btn-primary mt-4 mb-2" href="{{ path('app_association_edit', {'id': association.id}) }}">Edit</a>
	<a class="btn btn-primary mt-4 mb-2" href="{{ path('app_association_afficher') }}">Back To List</a>
	{{ include('association/_delete_form.html.twig') }}
{% endblock %}
